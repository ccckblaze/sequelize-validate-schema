{
  "name": "sequelize-validation-schema",
  "version": "0.0.0",
  "private": false,
  "engines": {
    "node": "8.5.0",
    "yarn": "1.0.2",
    "npm": "5.3.0"
  },
  "main": "dist/js/main/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild": "npm run clean",
    "tsc": "tsc",
    "build": "npm run tsc",
    "test": "export NODE_ENV=test && nyc mocha --opts src/ts/spec/mocha.opts"
  },
  "directories": {
    "test": "src/spec"
  },
  "nyc": {
    "include": [
      "src/ts/**/*.ts"
    ],
    "exclude": [
      "src/ts/spec/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html",
      "cobertura"
    ],
    "report-dir": "dist/report",
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.4",
    "pg": "^6",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.8.1",
    "sequelize-typescript": "^0.4.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.10",
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "concurrently": "^3.5.0",
    "cpx": "^1.5.0",
    "espower-typescript": "^8.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "mocha-multi": "^0.11.0",
    "nyc": "^11.2.1",
    "power-assert": "^1.4.4",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  },
  "optionalDependencies": {
    "fsevents": "^1.1.2"
  }
}
